// This is your Prisma schema file,

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id              Int     @id @default(autoincrement())
  name            String
  username        String  @unique
  password        String
  pin             String?
  local_area      String?
  coins_available Int     @default(0)
}
